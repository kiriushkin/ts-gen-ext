{
  "manifest_version": 3,
  "name": "TS Tools",
  "description": "All Rights Reserved",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "activeTab", "storage"],
  "icons": {
    "512": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/jira.js"],
      "matches": ["https://jira.lucky-team.pro/*"]
    }
  ]
}
